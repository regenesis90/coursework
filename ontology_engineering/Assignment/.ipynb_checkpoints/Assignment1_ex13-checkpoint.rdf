     <dc:title>Oracle</dc:title>
     <dc:year>13</dc:year>
   </Programming>
 </Skills>

 <Skills>
   <Programming rdf:ID="Java">
     <dc:title>Java</dc:title>
     <dc:year>3</dc:year>
   </Programming>
 </Skills>

 <Education>
